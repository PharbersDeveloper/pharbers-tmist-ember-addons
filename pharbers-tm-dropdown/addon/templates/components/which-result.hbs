{{!-- <div local-class="select"> --}}
{{#bs-dropdown local-class="title" tagName="span" as |dd|}}
    {{#dd.toggle}}{{data.current}}<span class="caret"></span>{{/dd.toggle}}
    {{#dd.menu local-class="ull" as |menu|}}
        {{#each data.selected as |selected|}}
            {{#menu.item}}
                {{#menu.link-to selected.url}}{{selected.text}}{{/menu.link-to}}
                {{!-- <a href={{selected.url}}>{{selected.text}}</a> --}}
            {{/menu.item}}
            {{menu.divider}}
        {{/each}}
    {{/dd.menu}}
{{/bs-dropdown}}
{{!-- </div> --}}
