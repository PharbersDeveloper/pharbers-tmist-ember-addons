{{!-- {{#mapping-components evaluationCard.component_data as |componentName component|}}
    {{component.component}}
{{/mapping-components}}--}}
{{#each data as |item|}}
    {{#mapping-components item as |componentName component|}}
        {{#if (eq componentName.componentName 'capability-radar')}}
            {{component.component data=radarData}}
        {{/if}}
        {{#if (eq componentName.componentName 'evaluation-title-card')}}
            {{component.component data=cardData}}
        {{/if}}
        {{#if (eq componentName.componentName 'capability-line')}}
            {{component.component data=lineData}}
        {{/if}}
    {{/mapping-components}}
{{/each}}
