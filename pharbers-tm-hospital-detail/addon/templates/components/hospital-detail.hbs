{{#mapping-components data.component_data as |componentName component|}}
    {{#if (eq componentName.componentName "target-detail-hospital-card")}}
        {{component.component}}
    {{/if}}
    {{#if (eq componentName.componentName "target-detail-medicines-card")}}
        {{component.component hospital_id=data.id uuid=uuid}}
    {{/if}}
{{/mapping-components}}
